    }

    .button-green {
        background-color: white;
        color: black;
        border: 2px solid green;
        padding: 10px 16px;
        text-align: center;
        margin: 4px 2px;
        transition-duration: 0.4s;
        cursor: pointer;
        border-radius: 4px;

    }
    .button:hover {
        background-color: green;
        color: white;
        border-radius: 4px;
    }

</style>
